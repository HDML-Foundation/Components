{
  "name": "HDML Components Environment",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "mounts": [
    "source=${localEnv:HOME}/.ssh,target=/home/.ssh,type=bind,consistency=cached,readonly"
  ],
  "postAttachCommand": "sh /workspaces/HDML-Components/scripts/init.sh",
  "customizations": {
    "vscode": {
      "settings": {},
      "extensions": ["ms-azuretools.vscode-docker", "gaborv.flatbuffers", "golang.go", "dbaeumer.vscode-eslint", "firsttris.vscode-jest-runner", "rvest.vs-code-prettier-eslint", "runem.lit-plugin", "github.vscode-github-actions"]
    }
  }
}